    <outputdir>bin/</outputdir>
    <outputname>makegen</outputname>
    <outputtype>executable</outputtype>
    <projectname>MakeGen</projectname>
    <srcdir>src/</srcdir>
  </configuration>
  <target>Release</target>
  <version>v1.3.4</version>
</makegen>
